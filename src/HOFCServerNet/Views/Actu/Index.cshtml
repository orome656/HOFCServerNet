@using HOFCServerNet.Data.Models;
@{ 
    List<Actu> actus = (List<Actu>)ViewData["actus"];
}
<div class="row">
    @foreach (Actu actu in actus)
    {
        <div class="col-xs-12 col-sm-6 col-md-4 col.lg-3">
            <img src="@actu.ImageURL" class="img-responsive img-rounded"/>
            <h2>@actu.Titre</h2>
            <div>@actu.Texte</div>
            <div><a href="@actu.URL">Détails</a></div>
        </div>
    }
</div>